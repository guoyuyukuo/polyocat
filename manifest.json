{
  "manifest_version": 2,
  "name": "Polyocat",
  "version": "0.0.0.3",
  "description": "Github in your language",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["polyocat.js"]
    }
  ],
  "web_accessible_resources": [
    "translations/*.json"
  ]
}